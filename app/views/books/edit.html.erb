<h1>Editing Book</h1>
<p>Find me in app/views/books/edit.html.erb</p>

  <%= form_with model: @book do |f| %>
    <p>Title</p>
    <%= f.text_field :title %>
    <p>Body</p>
    <%= f.text_field :body %>
    <%= f.submit :"Update Book"%>
  <%end%>
  
  <a href="/books/:id">Show</a>|<a href="/books">Back</a>